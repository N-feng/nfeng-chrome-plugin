{
	"manifest_version": 2,
	"name": "nfeng-chrome-plugin",
	"version": "1.0.0",
	"background": {
		"scripts": [
			"js/background.js"
		]
	},
	"browser_action": {
		"default_popup": "/popup/popup.html"
	},
	"permissions": [
		"declarativeContent",
		"contextMenus",
		"tabs",
		"downloads",
		"storage"
	],
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"js": [
				"lib/jquery-1.11.1.js",
				"lib/jquery.formautofill.js",
				"content_js/content-script.js",
				"content_js/common.js",
				"content_js/imageDownloader.js",
				"content_js/autofill.js"
			],
			"css": [
				"css/custom.css"
			],
			"run_at": "document_start"
		}
	],
	"web_accessible_resources": [
		"web_js/inject.js"
	],
	"omnibox": {
		"keyword": "nf"
	},
	"devtools_page": "devtools.html"
}